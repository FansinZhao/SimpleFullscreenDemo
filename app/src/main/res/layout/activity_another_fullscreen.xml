<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_another_fullscreen"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#b61962"
    tools:context="com.zhaofeng.simplefull.AnotherFullscreenActivity">

    <TextView
        android:id="@+id/fullscreen_another_content"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:keepScreenOn="true"
        android:text="@string/dummy_another_content"
        android:textColor="#33b5e5"
        android:textSize="50sp"
        android:textStyle="bold" />
</RelativeLayout>
